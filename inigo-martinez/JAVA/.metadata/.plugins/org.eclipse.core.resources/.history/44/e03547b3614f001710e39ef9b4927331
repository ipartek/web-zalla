package prueba;

// La armadura no funciona correctamente. Deberia de reducir el daño, no sumarse directamente.

public class Orco extends Unidad {

	public static int m(int p) {
	}

	int PV = (int) Math.floor(Math.random() * (20) + 1);
	int danio = (int) Math.floor(Math.random() * (15) + 1);
	int armadura = (int) Math.floor(Math.random() * (1) + 1);

	public Orco() {
		super("Orco", PV, danio, armadura);
	}
}